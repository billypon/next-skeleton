{
  "name": "next-skeloton",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "build": "rm -rf .next && next build",
    "postbuild": "rm -rf dist && next export -o dist && bin/postbuild",
    "build:ssr": "rm -rf .next && next build",
    "postbuild:ssr": "rm -rf dist && mkdir dist && cp -a .next dist && bin/postbuild ssr",
    "start:prod": "next start",
    "lint": "eslint pages utils --ext .ts,.tsx -f codeframe",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "babel-plugin-transform-react-twist": "0.1.9",
    "dotenv-load": "2.0.0",
    "next": "11.1.3",
    "qs": "6.9.0",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-router-dom": "5.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@billypon/react-decorator": "0.1.4",
    "@types/next": "8.0.6",
    "@types/node": "12.7.8",
    "@types/qs": "6.5.3",
    "@types/react": "16.9.4",
    "@types/react-dom": "16.9.1",
    "@types/react-router-dom": "5.1.0",
    "@typescript-eslint/eslint-plugin": "2.3.2",
    "@typescript-eslint/parser": "2.3.2",
    "@zeit/next-stylus": "1.0.1",
    "babel-loader": "8.0.6",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-loader": "3.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.15.0",
    "eslint-plugin-react-hooks": "2.1.1",
    "next-compose-plugins": "2.2.0",
    "next-images": "1.2.0",
    "next-transpile-modules": "2.3.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "patch-package": "6.2.0",
    "pug-as-jsx-loader": "1.0.79",
    "style-loader": "1.0.0",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "typescript": "3.6.3"
  }
}
